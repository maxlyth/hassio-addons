{
  "name": "Shelly Admin",
  "version": "0.3.0",
  "slug": "shelly-admin",
  "description": "Admin console for Shelly IoT devices",
  "url": "https://github.com/maxlyth/shelly-admin",
  "stage": "experimental",
  "advanced": true,
  "ingress": true,
  "ingress_port": 0,
  "apparmor": true,
  "startup": "application",
  "panel_icon": "mdi:alpha-s-circle",
  "homeassistant": "0.92.0b2",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "init": true,
  "host_network": true,
  "image": "maxlyth/shelly-admin-{arch}",
  "options": {
    "dark_mode": false,
    "shelly_auth": {
      "username": "",
      "password": ""
    }
  },
  "schema": {
    "dark_mode": "bool",
    "shelly_auth": {
      "username": "str",
      "password": "password"
    }
  }
}