{
  "name": "Shelly Admin",
  "version": "dev",
  "slug": "shelly-admin",
  "description": "Admin console for Shelly IoT devices",
  "url": "https://github.com/maxlyth/shelly-admin-express",
  "advanced": true,
  "ingress": true,
  "ingress_port": 4081,
  "startup": "services",
  "panel_icon": "mdi:docker",
  "homeassistant": "0.92.0b2",
  "arch": ["aarch64", "amd64", "armhf", "armv7"],
  "init": false,
  "map": [
    "config:rw",
  ],
  "image": "maxlyth/shelly-admin-{arch}",
  "ports": {
    "4081/tcp": 3000
  },
  "ports_description": {
    "4081/tcp": "Web interface"
  },
  "hassio_api": true,
  "hassio_role": "manager",
  "homeassistant_api": true,
  "host_network": true,
  "auth_api": true,
  "privileged": [
    "NET_ADMIN"
  ],
  "apparmor": false,
  "schema": false
}
